<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<VBox maxHeight="-Infinity" maxWidth="300" minHeight="-Infinity" minWidth="200.0" prefWidth="200.0"
      spacing="8.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="at.fhtw.swen2.presentation.view.TourView">

    <children>
        <Label text="%tab.demo.title">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Separator prefWidth="200.0" />
        <Text text="Name"/>
        <TextField fx:id="nameTextField"/>
        <Text text="Description"/>
        <TextField fx:id="descriptionTextField"/>
        <Text text="From"/>
        <TextField fx:id="fromTextField"/>
        <Text text="To"/>
        <TextField fx:id="toTextField"/>
        <Text text="Transport"/>
        <ChoiceBox fx:id="transportTypeChoiceBox"/>
        <Button fx:id="createButton" onAction="#submitButtonAction" text="insert"/>
        <Separator prefWidth="200.0" />
        <Text fx:id="feedbackText" />
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</VBox>
